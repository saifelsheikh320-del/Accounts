{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.cjs",
            "use": "@vercel/node"
        },
        {
            "src": "dist/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist/index.cjs"
        },
        {
            "src": "/auth/(.*)",
            "dest": "dist/index.cjs"
        },
        {
            "src": "/(.*)",
            "dest": "dist/public/index.html"
        }
    ]
}